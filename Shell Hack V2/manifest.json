{
  "manifest_version": 2,
  "name": "ShellHack",
  "version": "4.3.1",
  "author": "TDStuart",
  "description": "by TDStuart",
  "content_scripts": [{
    "js": ["content.js"],
    "matches": ["https://shellshock.io/*"]
